{
  "name": "musement/phpstan-rules",
  "type": "library",
  "description": "Additional phpstan rules used @ Musement.",
  "keywords": [
    "phpstan",
    "phpstan-rules",
    "phpstan-opinionated-rules",
    "musement"
  ],
  "homepage": "https://github.com/musement/phpstan-rules",
  "license": "MIT",
  "authors": [
    {
      "name": "Simone Fumagalli",
      "email": "simone.fumagalli@musement.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "nikic/php-parser": "^4.1.0",
    "phpstan/phpstan": "~0.10.5 || ~0.11.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.1.0",
    "symplify/easy-coding-standard": "^5.4"
  },
  "config": {
    "preferred-install": "dist"
  },
  "autoload": {
    "psr-4": {
      "Musement\\PHPStan\\Rules\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Musement\\PHPStan\\Rules\\Test\\": "test/"
    }
  },
  "support": {
    "issues": "https://github.com/musement/phpstan-rules/issues",
    "source": "https://github.com/musement/phpstan-rules"
  }
}
