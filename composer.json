{
  "name": "hpatoio/jsg",
  "type": "library",
  "description": "Generate an object from a json schema definition and vice versa",

  "keywords": [
    "json-schema", "json"
  ],
  "homepage": "https://github.com/hpatoio/jsg",
  "license": "MIT",
  "authors": [
    {
      "name": "Simone Fumagalli",
      "email": "simone@iliveinperego.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "jms/serializer": "^3.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.1.0",
    "symplify/easy-coding-standard": "^5.4"
  },
  "config": {
    "preferred-install": "dist"
  },
  "autoload": {
    "psr-4": {
      "Hpatoio\\JsonSchema\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Hpatoio\\JsonSchema\\Test\\": "test/"
    }
  },
  "support": {
    "issues": "https://github.com/hpatoio/jsg/issues",
    "source": "https://github.com/hpatoio/jsg"
  }
}
